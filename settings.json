{
  // * common
  "update.showReleaseNotes": false,
  // * javascript/typescript
  "javascript.preferences.importModuleSpecifier": "relative",
  "javascript.preferences.importModuleSpecifierEnding": "minimal",
  "javascript.preferences.quoteStyle": "single",
  "typescript.preferences.importModuleSpecifier": "relative",
  "typescript.suggest.paths": false,
  "typescript.tsserver.trace": "off",
  "typescript.suggest.autoImports": true,
  "diffEditor.codeLens": true,
  "typescript.implementationsCodeLens.enabled": true,
  "typescript.disableAutomaticTypeAcquisition": true,
  // * window
  "editor.rulers": [80, 120],
  "window.title": "${activeEditorLong}${separator}${rootName}",
  "window.newWindowDimensions": "inherit",
  // * workbench
  "workbench.colorTheme": "Tokyo Night Storm",
  "workbench.editor.decorations.badges": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.labelFormat": "default",
  "workbench.editor.tabSizing": "shrink",
  "workbench.editor.untitled.hint": "hidden",
  "workbench.editor.showIcons": false,
  "workbench.editor.enablePreview": false,
  "workbench.editor.tabCloseButton": "off", // ???
  "workbench.editor.splitOnDragAndDrop": false,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "none",
  "workbench.activityBar.visible": false,
  // * explorer
  "explorer.autoReveal": false,
  "explorer.sortOrder": "type",
  // * editor
  "editor.cursorSmoothCaretAnimation": true,
  "editor.cursorStyle": "line-thin",
  "editor.fontFamily": "JetBrains Mono",
  "editor.fontLigatures": true,
  "editor.fontSize": 12,
  "editor.fontWeight": "300",
  "editor.formatOnSave": true,
  "---prettier.useEditorConfig": false,
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "editor.lightbulb.enabled": false,
  "editor.lineHeight": 22,
  "editor.minimap.enabled": false,
  "editor.smoothScrolling": true,
  "editor.tabSize": 2,
  "editor.wordSeparators": "`~!@#$%^&*()-_=+[{]}\\|;:'\",.<>/?",
  "editor.wordWrapColumn": 100,
  "editor.codeLens": true,
  "editor.cursorBlinking": "solid",
  // * editor suggestions
  "editor.snippetSuggestions": "inline",
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "editor.suggest.localityBonus": true,
  "editor.quickSuggestions": {
    "other": true,
    "comments": false,
    "strings": false
  },
  "editor.quickSuggestionsDelay": 100,
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.suggest.showWords": false,
  "editor.suggest.showDeprecated": false,
  "editor.wordBasedSuggestions": false,
  "editor.folding": false,
  "editor.lineNumbers": "off",
  "editor.autoClosingBrackets": "beforeWhitespace", // ???
  "editor.codeLensFontFamily": "JetBrains Mono",
  "editor.colorDecorators": true,
  "editor.formatOnSaveMode": "file",
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.gotoLocation.multipleDeclarations": "goto",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleTypeDefinitions": "peek",
  "editor.hover.delay": 2000,
  // * emmet
  "emmet.showSuggestionsAsSnippets": false,
  "emmet.triggerExpansionOnTab": false,
  "emmet.includeLanguages": {
    "nunjucks": "html"
  },
  "emmet.excludeLanguages": ["javascriptreact", "typescriptreact"],
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  // * colors
  "editor.tokenColorCustomizations": {
    "[Tokyo Night Storm]": {
      "comments": "#3070de"
    }
  },
  "workbench.colorCustomizations": {
    "[Tokyo Night Light]": {
      "editor.background": "#d9d9dd",
      "sideBar.background": "#d6d6db",
      "terminal.background": "#d6d6db",
      "editor.lineHighlightBackground": "#d6d6db",
      "list.activeSelectionBackground": "#ceced4"
    },
    "[Tokyo Night Storm]": {
      "editorRuler.foreground": "#162539",
      "activityBar.activeBackground": "#0a2957",
      "activityBar.inactiveForeground": "#3f4c63",
      "activityBar.background": "#0b1b2f",
      "activityBar.foreground": "#869bb9",
      "activityBar.border": "#0b1b2f",
      "activityBar.activeBorder": "#0a2957",
      "activityBarBadge.background": "#0b1b2f",
      "activityBarBadge.foreground": "#869bb9",
      // sidbar
      "sideBar.background": "#0b1b2f",
      "sideBar.foreground": "#9da0b9",
      "sideBarSectionHeader.background": "#0b1b2f",
      "sideBarSectionHeader.border": "#081422",
      // list
      "list.activeSelectionBackground": "#3740b6",
      "list.inactiveSelectionBackground": "#061526",
      "list.hoverBackground": "#061526",

      // editor popup
      "editorSuggestWidget.background": "#0b1b2f",
      "editorSuggestWidget.highlightForeground": "#6473e6",
      "editorSuggestWidget.selectedBackground": "#3740b6",

      "editorHoverWidget.background": "#0b1b2f",
      "dropdown.background": "#0b1b2f",
      "dropdown.foreground": "#2e4663",
      // editor
      "editor.background": "#19273f",
      "editor.lineHighlightBackground": "#1a2d49",
      // "editorCodeLens.foreground": "#3070de",
      "editorGroupHeader.tabsBackground": "#0b1b2f",
      "editorGroupHeader.noTabsBackground": "#0b1b2f",
      // breadcrumb
      "breadcrumb.background": "#0b1b2f",
      // input
      "input.background": "#091627",
      // terminal
      "statusBar.background": "#050e18",
      "terminal.background": "#0b1b2f",
      "panel.background": "#0b1b2f",
      // tabs
      "tab.activeBackground": "#142642",
      "tab.activeBorder": "#142642",
      "tab.inactiveBackground": "#0b1b2f",
      "tab.activeForeground": "#6c7c91",
      // panel
      "panelTitle.activeBorder": "#3740b6",
      "panelTitle.inactiveForeground": "#2e4663",
      "panelTitle.activeForeground": "#6c7c91",

      // scrollbar
      "scrollbarSlider.background": "#22344b",
      "scrollbarSlider.hoverBackground": "#2e4663",
      "scrollbarSlider.activeBackground": "#2e4663"
    }
  },
  // * plugins
  "gitlens.showWhatsNewAfterUpgrades": false,
  "gitlens.showWelcomeOnInstall": false,
  "gitlens.hovers.enabled": true,
  "gitlens.statusBar.enabled": false,
  "window.zoomLevel": 2,
  "gitPrefix.patternIgnoreCase": true,
  "gitPrefix.replacement": "[$1] ",
  "gitlens.codeLens.enabled": false,
  "gitlens.codeLens.scopes": ["document", "containers", "blocks"],
  "auto-close-tag.SublimeText3Mode": true,
  "autoimport.autoComplete": false,
  "postfix.customTemplates": [
    {
      "name": "useState",
      "body": "const [{{expr}}, set{{expr:capitalize}}] = useState($1)",
      "description": "React useState",
      "when": []
    },
    {
      "name": "/>",
      "body": "<{{expr}} />",
      "description": "<{{expr}} />",
      "when": []
    },
    {
      "name": ">",
      "body": "<$RANDOM {{expr}}>$1</{{expr}}>",
      "description": "<{{expr}} />",
      "when": []
    }
  ],
  "surround.showOnlyUserDefinedSnippets": true,
  "surround.with.arrowFunction": {
    "label": "arrowFunction",
    "description": "const $name = ($params) => { ... }",
    "disabled": false,
    "snippet": "const ${1:fnName} = (${2:params}) => {\n\t$TM_SELECTED_TEXT\n}$0"
  },
  "surround.custom": {
    "jsxMap": {
      "label": "jsxMap",
      "description": "",
      "disabled": false,
      "snippet": "{$1.map((it) => {\n return (\n\t$TM_SELECTED_TEXT\n)\n})}"
    }
  },
  "jest.enableSnapshotUpdateMessages": false,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "cSpell.userWords": ["devtool", "firestore", "testid"],
  "material-icon-theme.activeIconPack": "react_redux",
  "material-icon-theme.folders.associations": {
    "elm": "functions",
    "session": "core",
    "proxy": "ngrx-store",
    "nginx": "ngrx-store"
  },
  "breadcrumbs.enabled": false,
  // * activitusbar
  "activitusbar.activeColour": "#9da0b9",
  "activitusbar.inactiveColour": "#9da0b9",
  "activitusbar.showSourceControlCounter": true,
  "activitusbar.toggleSidebar": false,
  "activitusbar.views": [
    {
      "name": "command.workbench.action.previousSideBarView",
      "codicon": "arrow-left"
    },
    {
      "name": "command.workbench.action.nextSideBarView",
      "codicon": "arrow-right"
    },
    {
      "name": "explorer",
      "codicon": "explorer-view-icon"
    },
    {
      "name": "search",
      "codicon": "search-view-icon"
    },
    {
      "name": "scm",
      "codicon": "source-control-view-icon"
    },
    {
      "name": "command.workbench.view.extension.gitlens",
      "codicon": "git-merge",
      "label-": "gitLens"
    },
    {
      "name": "command.workbench.view.extension.bookmarks",
      "codicon": "bookmark",
      "tooltip": "Bookmarks"
    },
    {
      "name": "command.workbench.view.extension.project-manager",
      "codicon": "folder",
      "tooltip": "Bookmarks"
    },
    {
      "name": "extensions",
      "codicon": "extensions"
    },
    {
      "name": "settings",
      "codicon": "gear"
    },
    {
      "name": "command.workbench.action.togglePanel",
      "codicon": "terminal"
    }
  ],
  "explorer.compactFolders": false,
  "material-icon-theme.hidesExplorerArrows": true,
  "jest.autoRun": {
    "watch": false,
    "onSave": "test-file"
  },

  "npm.packageManager": "yarn",
  "eslint.packageManager": "yarn",
  "explorer.openEditors.visible": 0,
  "files.autoSave": "onFocusChange"
}
