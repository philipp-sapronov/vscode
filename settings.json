{
  // * common
  "update.showReleaseNotes": false,
  // * javascript/typescript
  "javascript.preferences.importModuleSpecifier": "project-relative",
  "javascript.preferences.importModuleSpecifierEnding": "minimal",
  "javascript.preferences.quoteStyle": "single",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "typescript.suggest.paths": false,
  "typescript.tsserver.trace": "off",
  "typescript.suggest.autoImports": true,
  "diffEditor.codeLens": true,
  "typescript.implementationsCodeLens.enabled": true,
  "typescript.disableAutomaticTypeAcquisition": true,
  // * window
  "window.title": "${rootName}",
  "window.newWindowDimensions": "inherit",
  // * workbench
  "workbench.colorTheme": "Tokyo Night Storm",
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.labelFormat": "short",
  "workbench.editor.tabSizing": "shrink",
  "workbench.editor.showIcons": false,
  "workbench.editor.enablePreview": false,
  "workbench.editor.tabCloseButton": "right",
  "workbench.editor.splitOnDragAndDrop": false,
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.value": 4,
  "workbench.editor.pinnedTabSizing": "compact",
  // "workbench.editor.openPositioning": "first",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "none",
  // * explorer
  "explorer.autoReveal": false,
  "explorer.sortOrder": "type",
  // * editor
  "editor.rulers": [80, 120],
  "editor.cursorSmoothCaretAnimation": false,
  "editor.cursorStyle": "block",
  "editor.fontFamily": "JetBrains Mono",
  "editor.fontLigatures": false,
  "editor.fontSize": 12,
  "editor.fontWeight": "300",
  "editor.formatOnSave": true,
  "prettier.useEditorConfig": false,
  "prettier.arrowParens": "always",
  "editor.bracketPairColorization.enabled": true,
  "editor.lightbulb.enabled": false,
  "editor.lineHeight": 22,
  "editor.smoothScrolling": true,
  "editor.tabSize": 2,
  "editor.wordSeparators": "`~!@#$%^&*()-_=+[{]}\\|;:'\",.<>/?",
  "editor.wordWrapColumn": 100,
  "editor.codeLens": true,
  // "editor.cursorWidth": 20,
  "editor.cursorBlinking": "solid",

  // * editor suggestions
  "editor.snippetSuggestions": "bottom",
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "editor.suggest.localityBonus": true,
  "editor.quickSuggestions": {
    "other": true,
    "comments": false,
    "strings": false
  },
  "editor.quickSuggestionsDelay": 100,
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.suggest.showWords": false,
  "editor.suggest.showDeprecated": false,
  "editor.folding": false,
  "editor.lineNumbers": "off",
  "editor.autoClosingBrackets": "beforeWhitespace",
  "editor.codeLensFontFamily": "JetBrains Mono",
  "editor.colorDecorators": true,
  "editor.formatOnSaveMode": "file",
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.gotoLocation.multipleDeclarations": "goto",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleTypeDefinitions": "peek",
  "editor.hover.delay": 500,
  // * emmet
  "emmet.showSuggestionsAsSnippets": false,

  "emmet.triggerExpansionOnTab": true,
  "emmet.includeLanguages": {
    "nunjucks": "html",
    "javascript": "javascriptreact",
    "typescript": "typescriptreact"
  },

  "files.associations": {
    "*.css": "postcss"
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  // * colors
  "editor.tokenColorCustomizations": {
    "[Tokyo Night Storm]": {
      "comments": "#3070de"
    }
  },
  "workbench.colorCustomizations": {
    "[GitHub Light Default]": {
      "statusBar.background": "#050e18",
      "statusBar.foreground": "#9da0b9"
    },
    "[Tokyo Night Light]": {
      "editor.background": "#d9d9dd",
      "sideBar.background": "#d6d6db",
      "terminal.background": "#d6d6db",
      "editor.lineHighlightBackground": "#d6d6db",
      "list.activeSelectionBackground": "#ceced4"
    },
    "[Tokyo Night Storm]": {
      "editorRuler.foreground": "#162539",
      "activityBar.activeBackground": "#0a2957",
      "activityBar.inactiveForeground": "#3f4c63",
      "activityBar.background": "#0b1b2f",
      "activityBar.foreground": "#869bb9",
      "activityBar.border": "#0b1b2f",
      "activityBar.activeBorder": "#0a2957",
      "activityBarBadge.background": "#0b1b2f",
      "activityBarBadge.foreground": "#869bb9",
      "editorCursor.foreground": "#869bb9",
      "editorCursor.background": "#000",
      // sidbar
      "sideBar.background": "#0b1b2f",
      "sideBar.foreground": "#9da0b9",
      "sideBarSectionHeader.background": "#0b1b2f",
      "sideBarSectionHeader.border": "#081422",
      // list
      "list.activeSelectionBackground": "#3740b6",
      "list.inactiveSelectionBackground": "#061526",
      "list.hoverBackground": "#061526",
      // editor popup
      "editorSuggestWidget.background": "#0b1b2f",
      "editorSuggestWidget.highlightForeground": "#6473e6",
      "editorSuggestWidget.selectedBackground": "#3740b6",
      "editorHoverWidget.background": "#0b1b2f",
      "dropdown.background": "#0b1b2f",
      "dropdown.foreground": "#2e4663",
      // editor
      "editor.background": "#19273f",
      "editor.lineHighlightBackground": "#1a2d49",
      // "editorCodeLens.foreground": "#3070de",
      "editorGroupHeader.tabsBackground": "#0b1b2f",
      "editorGroupHeader.noTabsBackground": "#0b1b2f",
      // breadcrumb
      "breadcrumb.background": "#0b1b2f",
      // input
      "input.background": "#091627",
      // terminal
      "statusBar.background": "#040b13",
      "statusBar.foreground": "#597293",
      "terminal.background": "#0b1b2f",
      "panel.background": "#0b1b2f",
      // tabs
      "tab.activeBackground": "#142642",
      "tab.activeBorder": "#142642",
      "tab.inactiveBackground": "#0b1b2f",
      "tab.activeForeground": "#6c7c91",
      // panel
      "panelTitle.activeBorder": "#3740b6",
      "panelTitle.inactiveForeground": "#2e4663",
      "panelTitle.activeForeground": "#6c7c91",
      // scrollbar
      "scrollbarSlider.background": "#22344b",
      "scrollbarSlider.hoverBackground": "#2e4663",
      "scrollbarSlider.activeBackground": "#2e4663"
    }
  },
  // * plugins
  "gitlens.showWhatsNewAfterUpgrades": false,
  "gitlens.showWelcomeOnInstall": false,
  "gitlens.hovers.enabled": true,
  "gitlens.statusBar.enabled": false,
  "window.zoomLevel": 2,
  "gitPrefix.patternIgnoreCase": true,
  "gitPrefix.replacement": "[$1] ",
  "gitlens.codeLens.enabled": false,
  "gitlens.codeLens.scopes": ["document", "containers", "blocks"],
  "jest.enableSnapshotUpdateMessages": false,
  "cSpell.userWords": [
    "devtool",
    "firestore",
    "lookback",
    "middlewares",
    "testid",
    "unmount",
    "unmounts"
  ],
  "material-icon-theme.activeIconPack": "react_redux",
  "material-icon-theme.folders.associations": {
    "elm": "functions",
    "session": "core",
    "proxy": "ngrx-store",
    "nginx": "ngrx-store"
  },
  // * activitusbar
  // "activitusbar.alignment": "Right",
  "activitusbar.activeColour": "#597293",
  "activitusbar.inactiveColour": "#597293",
  "activitusbar.showSourceControlCounter": true,
  "activitusbar.toggleSidebar": false,
  "macros": {
    "toggleNpmView": [
      { "command": "workbench.view.explorer" },
      { "command": "npm.toggleVisibility" }
    ]
  },
  "activitusbar.views": [
    {
      "label": "ü™Å Explorer",
      "name": "explorer",
      "codicon": "cebab",
      "tooltip": "Explorer"
    },
    {
      "name": "search",
      // "codicon": "cebab",
      "codicon": "search-view-icon",
      "tooltip": "Search",
      "label": "Search "
    },
    {
      "label": "Git",
      "name": "scm",
      "codicon": "list-tree",
      "tooltip": "Git"
    },
    {
      // "id": "gitlens",
      "name": "command.workbench.view.extension.gitlens",
      "codicon": "source-control-view-icon",
      "tooltip": "Git Lens",
      "label": "Git Lens"
      // "alignment": "left",
      // "priority": 11
    },
    {
      "name": "command.workbench.view.extension.test",
      "tooltip": "Jest",
      "codicon": "test-view-icon",
      "label": "Jest"
    },
    {
      "id": "terminal",
      "label": "ü¶ã  ∆¨ E ∆¶ M I ∆ù Œõ ≈Å  ",
      "name": "command.workbench.action.togglePanel",
      "tooltip": "Terminal",
      "codicon": "cebab",
      "alignment": "left",
      "priority": 10
    },
  ],
  "statusbar_command.commands": [
    // {
    //   "id": "terminal",
    //   "label": "Terminal",
    //   "command": "workbench.action.togglePanel",
    //   "tooltip": "Terminal",
    //   "text": "$(terminal) Terminal",
    //   "alignment": "left",
    //   "priority": 10,
    // },

    // on the right
    {
      "alignment": "right",
      "command": "workbench.action.openWorkspaceSettings",
      "id": "json_settings",
      "name": "JSON Settings",
      "text": "$(gear)",
      "tooltip": "JSON Settings"
    },
    {
      "alignment": "right",
      "command": "workbench.view.extensions",
      "id": "extensions",
      "name": "Extensions",
      "text": "$(extensions)",
      "tooltip": "Extensions"
    }
  ],
  "explorer.compactFolders": false,
  "material-icon-theme.hidesExplorerArrows": true,
  "jest.autoRun": {
    "watch": false,
    "onSave": "test-file"
  },
  "npm.scriptExplorerAction": "run",
  "npm.packageManager": "yarn",
  "eslint.packageManager": "yarn",
  "explorer.openEditors.visible": 0,
  "files.autoSave": "onFocusChange",
  "regex-previewer.enableCodeLens": false,
  "tabnine.experimentalAutoImports": true,
  // "autoimport.filesToScan": "**/*.{ts,tsx}"
  "auto-close-tag.enableAutoCloseSelfClosingTag": true,
  "auto-close-tag.SublimeText3Mode": true,
  "autoimport.autoComplete": true,
  "auto-close-tag.insertSpaceBeforeSelfClosingTag": true,
  "auto-close-tag.activationOnLanguage": [
    "xml",
    "javascriptreact",
    "typescriptreact",
    "markdown",
    "HTML (EEx)",
    "HTML (Eex)"
  ],
  // Rest Client
  "rest-client.enableCustomVariableReferencesCodeLens": false,
  "rest-client.rememberCookiesForSubsequentRequests": false,
  "breadcrumbs.icons": false,
  "breadcrumbs.symbolPath": "off",
  "jest.enableSnapshotPreviews": true,
  "editor.parameterHints.enabled": false,
  "editor.inlayHints.fontSize": 100,
  "editor.inlayHints.enabled": false,
  "editor.scrollbar.horizontalScrollbarSize": 10,
  "editor.scrollbar.verticalScrollbarSize": 10,
  "workbench.editor.wrapTabs": true,
  "terminal.integrated.defaultLocation": "editor",
  "workbench.productIconTheme": "material-product-icons",
  "workbench.editor.decorations.badges": false,
  "editor.suggest.insertMode": "replace",
  "tabnine.disable_file_regex": ["[.]css$"],
  "workbench.activityBar.visible": false,
  "editor.minimap.enabled": false,
  "editor.inlineSuggest.enabled": false,
  "editor.guides.bracketPairs": true,
  // ESLINT
  "editor.codeActionsOnSave": {
    // "source.organizeImports": true,
    "source.fixAll.eslint": true
  },
  "eslint.codeAction.showDocumentation": {
    "enable": true
  },
  "typescript.autoClosingTags": true,
  "javascript.autoClosingTags": true,
  "typescript.preferences.jsxAttributeCompletionStyle": "auto",
  "javascript.preferences.jsxAttributeCompletionStyle": "auto",
  // inlay hints
  "javascript.referencesCodeLens.enabled": true,
  "javascript.inlayHints.variableTypes.enabled": true,
  "typescript.referencesCodeLens.enabled": true,
  "typescript.inlayHints.variableTypes.enabled": true,
  "redhat.telemetry.enabled": false

  // "javascript.inlayHints.parameterNames.enabled": "all",
  // "typescript.inlayHints.variableTypes.enabled": true,
  // "typescript.inlayHints.parameterNames.enabled": "literals",
  // "typescript.inlayHints.parameterTypes.enabled": true,
}
